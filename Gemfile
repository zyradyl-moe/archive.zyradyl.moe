source 'https://rubygems.org'

require 'json'
require 'open-uri'
versions = JSON.parse(open('https://pages.github.com/versions.json').read)

gem 'nokogiri'
gem 'github-pages', versions['github-pages']
gem 'github-linguist'
gem 'github-markdown'
gem 'RedCloth'
gem 'gemoji'
gem 'escape_utils'
gem 'sanitize'
gem 'email_reply_parser'
gem 'jekyll-redirect-from'
gem 'jekyll-sitemap'
gem 'jemoji'
gem 'html-proofer'
gem 'rake'
gem 'launchy'
gem 'rubygems-bundler'
gem 'travis'
